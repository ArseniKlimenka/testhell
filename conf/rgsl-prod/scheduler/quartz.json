{
    "Quartz": {
        "quartz.scheduler.instanceId": "AUTO",
        "quartz.scheduler.instanceName": "AdInsure",
        "quartz.threadPool.type": "Quartz.Simpl.SimpleThreadPool, Quartz",
        "quartz.threadPool.threadCount": "3",
        "quartz.jobStore.useProperties": "true",
        "quartz.serializer.type": "json",
        "quartz.jobStore.misfireThreshold": "1000",
        "quartz.jobStore.type": "Quartz.Impl.AdoJobStore.JobStoreTX, Quartz",
        "quartz.jobStore.lockHandler.type": "Quartz.Impl.AdoJobStore.UpdateLockRowSemaphore, Quartz",
        // "quartz.jobStore.driverDelegateType": "",
        "quartz.jobStore.clustered": "true",
        "quartz.jobStore.tablePrefix": "QRTZ_",
        "quartz.jobStore.dataSource": "sqlJobStore",
        // "quartz.dataSource.sqlJobStore.connectionString": "",
        // "quartz.dataSource.sqlJobStore.provider": "",
        "quartz.plugin.jobHistory.type": "Quartz.Plugin.History.LoggingJobHistoryPlugin, Quartz.Plugins",
        "quartz.plugin.triggHistory.type": "Quartz.Plugin.History.LoggingTriggerHistoryPlugin, Quartz.Plugins"
    }
}