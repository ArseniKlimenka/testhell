{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft.AspNetCore": "Warning"
        }
    },
    "appSettings": {
        "AdInsure": {
            "Healthcheck": {
              "Enabled": "true"
            },
            "Settings": {
                "OAuthIdentity": {
                    "IdentityEndpointUrl": "https://rgsl-rc-auth.adacta-fintech.com"
                }
            }
        }
