{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "properties": {
        "partyCode": {
            "type": "string",
            "description": "Party code"
        },
        "fullName": {
            "type": "string",
            "description": "Party full name"
        },
        "partyType": {
            "type": "string",
            "description": "Party type"
        },       
        "partyBankAccount": {
            "type": "string",
            "description": "Party bank account number"
        },
        "bankAccountBankCode": {
            "type": "string",
            "description": "Bank account's bank code"
        },
        "partyBankAccountName": {
            "type": "string",
            "description": "Party bank account name"
        },
        "bankAccountCurrencyCode": {
            "type": "string",
            "description": "Bank account's bank code"
        },
        "INN": {
            "type": "string",
            "description": "Party INN number"
        },
        "KPP": {
            "type": "string",
            "description": "Party KPP number"
        },
        "isNonResident": {
            "type": "boolean",
            "description": "Is party non resident"
        },
        "corrBankAccount": {
            "type": "string",
            "description": "Party's correspondant bank account"
        }
        
    }
}