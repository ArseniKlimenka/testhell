SELECT PE.EMAIL
FROM PTY.PARTY p 
JOIN PTY_IMPL.PARTY_HUB ph ON ph.PARTY_CODE = p.PARTY_CODE
JOIN PTY_IMPL.PARTY_EMAILS_SAT_LATEST pe ON pe.PARTY_EMAILS_HKEY = ph.PARTY_HKEY
WHERE p.PARTY_CODE = (SELECT VALUE FROM ORG.APPLICATION_USER_CLAIM
WHERE APPLICATION_USER_ID = @applicationUserId
AND CLAIM_TYPE = 'PartyCode' AND pe.IS_DELETED = 0)