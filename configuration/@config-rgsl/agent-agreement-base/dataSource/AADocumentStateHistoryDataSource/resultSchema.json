{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type":"object",
    "additionalProperties": false,
    "properties": {
        "validFrom": {
            "type": "string",
            "description": "Date-time of change to document's state."
        },
        "transition": {
            "type": "string",
            "description": "Name of the transition that changed the state (used to track the '$Create' 'transition' - to rename it to 'Draft')"
        },
        "stateCode": {
            "type": "string",
            "description": "Code of document state."
        },
        "state": {
            "type": "string",
            "description": "Name of document state."
        },
        "changedByUser": {
            "type": "string",
            "description": "Full name of the user that caused a transition to said state."
        },
        "documentNumber": {
            "type": "string",
            "description": "Number of document"
        }
    }
}
