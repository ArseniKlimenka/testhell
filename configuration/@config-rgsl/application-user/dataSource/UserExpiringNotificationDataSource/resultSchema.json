{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "additionalProperties": false,
    "required": [
        "applicationUserId",
        "username",
        "email",
        "expireDate"
    ],
    "properties": {
        "applicationUserId": {
            "type": "string"
        },
        "username": {
            "type": "string"
        },
        "email": {
            "type": "string"
        },
        "expireDate": {
            "type": "string",
            "aiFormat": "date"
        }
    }
}