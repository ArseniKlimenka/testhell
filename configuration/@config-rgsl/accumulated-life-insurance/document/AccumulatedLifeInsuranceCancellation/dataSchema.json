            "$ref": "component:TaxDeduction"
        },
        "technicalData": {
            "type":"object",
            "additionalProperties": true,
            "properties": {}
        },
        "tempTechnicalData": {
            "type":"object",
            "additionalProperties": true,
            "properties": {
                "inquiries": {
                    "$ref": "component:CancellationInquiries"
                }
            }
        },
        "productConfiguration": {
            "$ref": "component:ProductConfiguration"
        },
