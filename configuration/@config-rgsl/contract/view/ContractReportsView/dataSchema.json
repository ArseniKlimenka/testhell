                    },
                    "strategyDescription": {
                        "type": "string"
                    }
                }
            }
        },
        "purchaseDate": {
            "type": "string",
            "aiFormat": "date"
        },
        "preEquityReportDate": {
            "type": "string",
            "aiFormat": "date"
        },
        "preEquityRegistryDate": {
            "type": "string",
            "aiFormat": "date"
