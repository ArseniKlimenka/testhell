{
    "$schema": "http://json-schema.org/draft-04/schema",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "code": {
            "type": "string",
            "description": "Success code."
        },
        "message": {
            "type": "string",
            "description": "Additional description."
        },
        "validationErrors": {
            "type": "array",
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "code": {
                        "type": "string"
                    },
                    "message": {
                        "type": "string"
                    },
                    "dataPath": {
                        "type": "string"
                    }
                }
            }
        },
        "createdPolicyNumber": {
            "type": "string"
        },
        "configurationName": {
            "type": "string"
        },
        "hasTransitioned": {
            "type": "boolean"
        }
    }
}