{
    "$schema": "http://json-schema.org/draft-04/schema",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "contractNumber",
        "issueDate",
        "holderPartyCode",
        "phoneNumber"
    ],
    "properties": {
        "contractNumber": {
            "type": "string"
        },
        "issueDate": {
            "type": "string"
        },
        "holderPartyCode": {
            "type": "string"
        },
        "phoneNumber": {
            "type": "string"
        },
        "throwOnIntegrationError": {
            "type": "boolean"
        },
        "partnerBusinessCode": {
            "type": "string"
        },
        "documentStateCode": {
            "type": "string"
        },
        "policyHolderFirstName": {
            "type": "string"
        },
        "policyHolderMiddleName": {
            "type": "string"
        },
        "productDescription": {
            "type": "string"
        },
        "productCode": {
            "type": "string"
        },
        "ruleCode": {
            "type": "string"
        },
        "ruleDescription": {
            "type": "string"
        },
        "sourceType": {
            "type": "string"
        }
    }
}