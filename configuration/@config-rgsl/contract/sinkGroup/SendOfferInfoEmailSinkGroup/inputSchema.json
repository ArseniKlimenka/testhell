{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "contractId",
        "contractConfName",
        "policyType",
        "shouldSignAttachment"
    ],
    "properties": {
        "contractId": {
            "type": "string"
        },
        "contractConfName": {
            "type": "string"
        },
        "policyType": {
            "type": "string"
        },
        "productCode": {
            "type": "string"
        },
        "issueDate": {
            "type": "string",
            "aiFormat": "date"
        },
        "recipientAddress": {
            "type": "string"
        },
        "shouldSignAttachment": {
            "type": "boolean"
        },
        "issueFormCode": {
            "type": "string"
        },
        "documentStateCode": {
            "type": "string"
        }
    }
}