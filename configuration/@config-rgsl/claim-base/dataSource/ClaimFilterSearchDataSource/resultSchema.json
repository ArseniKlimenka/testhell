{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "claimNumber": {
            "type": "string"
        },
        "claimConf": {
            "type": "string"
        },
        "documentState": {
            "type": "string"
        },
        "documentStateCode": {
            "type": "string"
        },
        "contractNumber": {
            "type": "string"
        },
        "contractConfigurationName": {
            "type": "string"
        },
        "contractConfigurationVersion": {
            "type": "string"
        },
        "contractStateCode": {
            "type": "string"
        },
        "contractStateDescription": {
            "type": "string"
        },
        "insuredEvent": {
            "type":"object",
            "properties": {
                "insuredEventNumber": {
                    "type": "string"
                },
                "insuredEventDate": {
                    "type": "string",
                    "format": "date",
                    "aiFormat": "date"
                }
            }
        },
        "applicationInfo": {
            "type":"object",
            "properties": {
                "applicantFullName": {
                    "type": "string"
                }
            }
        },
        "insuredPersonInfo": {
            "type":"object",
            "properties": {
                "insuredPersonFullName": {
                    "type": "string"
                }
            }
        },
        "policyHolderInfo": {
            "type":"object",
            "properties": {
                "policyHolderFullName": {
                    "type": "string"
                }
            }
        }
    }
}