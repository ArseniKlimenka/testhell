{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "documentNumber": {
            "type": "string"
        },
        "documentState": {
            "type": "string"
        },
        "contractNumber": {
            "type": "string"
        },
        "insuredEventDate": {
            "type": "string",
            "format": "date",
            "aiFormat": "date"
        },
        "diagnosis": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                }
            }
        },
        "diagnosisNote": {
            "type": "string"
        },        
        "insuredEventType": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                }
            }
        },
        "insuredEventReason": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                }
            }
        }
    }
}