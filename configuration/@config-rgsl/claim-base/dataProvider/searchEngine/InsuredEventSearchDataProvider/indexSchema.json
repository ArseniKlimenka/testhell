{
    "$schema": "http://json-schema.org/draft-04/schema",
    "type": "object",
    "properties": {
        "contract": {
            "type": "object",
            "properties": {
                "number": {
                    "type": "string",
                    "mapping": {
                        "path": "#/properties/attributes/properties/contract/properties/number"
                    }
                },
                "externalNumber": {
                    "type": "string",
                    "mapping": {
                        "path": "#/properties/attributes/properties/contract/properties/externalNumber"
                    }
                },
                "configurationName": {
                    "type": "string",
                    "mapping": {
                        "path": "#/properties/attributes/properties/contract/properties/configurationName"
                    }
                },
                "isExternal": {
                    "type": "boolean",
                    "mapping": {
                        "path": "#/properties/attributes/properties/contract/properties/isExternal"
                    }
                }
            }
        },
        "eventDateInfo": {
            "type": "object",
            "properties": {
                "eventDate": {
                    "type": "string",
                    "aiFormat": "date",
                    "mapping": {
                        "path": "#/properties/attributes/properties/eventDateInfo/properties/eventDate"
                    }
                },
                "yearQuarter": {
                    "type": "integer",
                    "mapping": {
                        "path": "#/properties/attributes/properties/eventDateInfo/properties/yearQuarter"
                    }
                }
            }
        },
        "insuredEventType": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "string",
                    "mapping": {
                        "path": "#/properties/attributes/properties/insuredEventType/properties/code"
                    }
                },
                "description": {
                    "type": "string",
                    "mapping": {
                        "path": "#/properties/attributes/properties/insuredEventType/properties/description"
                    }
                }
            }
        },
        "insuredEventReason": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "string",
                    "mapping": {
                        "path": "#/properties/attributes/properties/insuredEventReason/properties/code"
                    }
                },
                "description": {
                    "type": "string",
                    "mapping": {
                        "path": "#/properties/attributes/properties/insuredEventReason/properties/description"
                    }
                }
            }
        },
        "diagnosis": {
            "type": "object",
            "properties": {
                "code": {
                    "type": "string",
                    "mapping": {
                        "path": "#/properties/attributes/properties/diagnosis/properties/code"
                    }
                },
                "description": {
                    "type": "string",
                    "mapping": {
                        "path": "#/properties/attributes/properties/diagnosis/properties/description"
                    }
                }
            }
        },
        "diagnosisNote": {
            "type": "string",
            "mapping": {
                "path": "#/properties/attributes/properties/diagnosisNote"
            }
        },
        "fixedExchangeRate": {
            "type": "number",
            "mapping": {
                "path": "#/properties/attributes/properties/fixedExchangeRate"
            }
        },
        "useFixedExchangeRate": {
            "type": "boolean",
            "mapping": {
                "path": "#/properties/attributes/properties/useFixedExchangeRate"
            }
        }
    }
}