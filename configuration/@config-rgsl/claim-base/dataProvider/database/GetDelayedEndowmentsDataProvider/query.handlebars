SELECT doc.UNIVERSAL_DOCUMENT_NUMBER AS ENDOWMENT_NUMBER,
	   art.CODE_NAME AS CONF_CODE_NAME
FROM BFX.UNIVERSAL_DOCUMENT doc
JOIN EWT_IMPL.EWT_HUB hub ON hub.ENDOWMENT_NUMBER = doc.UNIVERSAL_DOCUMENT_NUMBER
JOIN EWT_IMPL.EWT_APPLICANT_LINK aLink ON aLink.EWT_HKEY = hub.EWT_HKEY
JOIN EWT_IMPL.EWT_APPLICANT_SAT_LATEST aSat ON aSat.EWT_APPLICANT_HKEY = aLink.EWT_APPLICANT_HKEY AND aSat.IS_DELETED = 0
JOIN CFX.PUBLISHED_ARTIFACT art ON art.PUBLISHED_ARTIFACT_ID = doc.PUBLISHED_ARTIFACT_ID
JOIN CFG.PROCESS_STATE st ON st.PROCESS_STATE_ID = doc.STATE_ID
WHERE
st.CODE_NAME = 'AwaitingEndowmentDate'
AND aSat.EVENT_DATE <= @currentDate