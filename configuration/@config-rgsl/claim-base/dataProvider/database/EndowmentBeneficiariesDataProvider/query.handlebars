SELECT c.UNIVERSAL_DOCUMENT_NUMBER AS ENDOWMENT_NUMBER,
	   r.BENEFICIARY_PARTY_CODE,
	   r.BENEFICIARY_PT_CODE
FROM BFX.UNIVERSAL_DOCUMENT c
JOIN CFX.PUBLISHED_ARTIFACT art ON art.PUBLISHED_ARTIFACT_ID = c.PUBLISHED_ARTIFACT_ID
CROSS APPLY OPENJSON(c.BODY,'$.endowmentBeneficiaries')
 with  
 (    
     BENEFICIARY_PARTY_CODE NVARCHAR(50) N'lax $.partyCode',
	 BENEFICIARY_PT_CODE NVARCHAR(50) N'lax $.beneficiaryPaymentType.code'
 ) r
 WHERE 
 art.CODE_NAME = 'Endowment'
 AND c.UNIVERSAL_DOCUMENT_NUMBER = @documentNumber