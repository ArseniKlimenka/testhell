SELECT ct.CODE,
	   ct.DESCRIPTION,
	   ct.TYPE_CODE 
FROM BFX_IMPL.INSURED_EVENT_REASON ct
WHERE 1 = 1
{{#if parameters.code}}
AND ct.CODE = @code
{{/if}}
{{#if parameters.typeCode}}
AND ct.TYPE_CODE = @typeCode
{{/if}}
{{#if parameters.description}}
AND ct.DESCRIPTION LIKE @description
{{/if}}
ORDER BY ct.CODE ASC