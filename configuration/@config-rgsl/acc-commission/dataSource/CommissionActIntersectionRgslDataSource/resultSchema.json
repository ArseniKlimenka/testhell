{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "additionalProperties": false,
    "properties": {
        "actId": {
            "type": "integer"
        },
        "actNo": {
            "type": "string"
        },
        "stateCode": {
            "$ref": "component:ActStateComponent"
        },
        "reportingPeriodFrom": {
            "type": "string",
            "aiFormat": "date"
        },
        "reportingPeriodTo": {
            "type": "string",
            "aiFormat": "date"
        },
        "productGroupInclude": {
            "type": "string"
        },
        "productGroupExclude": {
            "type": "string"
        },
        "includedProducts": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "excludedProducts": {
            "type": "array",
            "items": {
                "type": "string"
            }
        }
    }
}