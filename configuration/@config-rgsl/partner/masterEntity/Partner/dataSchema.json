{
    "$schema": "http://json-schema.org/draft-04/schema",
    "type": "object",
    "additionalProperties": false,
    "aiValidation": {
        "rule": "validatePartner"
    },
    "aiAudit": {
        "scope": "General"
    },
    "properties": {
        "partnerCode": {
            "type": "string",
            "aiTitle": "partnerCode"
        },
        "partnerType": {
            "type": "string",
            "enum": [
                "agent",
                "broker",
                "insuranceCompany"
            ],
            "aiTitle": "partnerType"
        },
        "partyId": {
            "type": "string"
        },
        "partyCode": {
            "type": "string",
            "aiTitle": "partyCode"
        },
        "partyDisplayName": {
            "type": "string",
            "aiTitle": "partyDisplayName"
        },
        "partyShortName": {
            "type": "string",
            "aiTitle": "partyDisplayName"
        }
    }
}