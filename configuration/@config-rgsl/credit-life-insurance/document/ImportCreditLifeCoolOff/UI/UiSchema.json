{
    "id": "UiSchema",
    "type": "Group",
    "formProperties": {
        "events": {
            "onLoad": "registerServerSideHandlers"
        }
    },
    "elements": [
        {
            "type": "GridLayout",
            "numberOfColumns": 4,
            "elements": [
                {
                    "type": "Component",
                    "name": "DocumentHeader",
                    "value": {
                        "$ref": "#"
                    }
                },
                {
                    "type": "Group",
                    "width": 4,
                    "elements": [
                        {
                            "type": "Group",
                            "elements": [
                                {
                                    "type": "GridLayout",
                                    "fillAvailableSpace": true,
                                    "elements": [
                                        {
                                            "type": "Group",
                                            "elements": [
                                                {
                                                    "type": "TabLayout",
                                                    "elementId": "tabs",
                                                    "options": {
                                                        "fixedPosition": true,
                                                        "scrollableTabs": true
                                                    },
                                                    "onInit": "prepareImportPricesTabs",
                                                    "elements": [
                                                        {
                                                            "type": "Tab",
                                                            "elements": [
                                                                {
                                                                    "type": "Section",
                                                                    "label": "GeneralInfo",
                                                                    "elements": [
                                                                        {
                                                                            "type": "Group",
                                                                            "elements": [
                                                                                {
                                                                                    "type": "FileUpload",
                                                                                    "label": "FileSelection",
                                                                                    "readOnly": true,
                                                                                    "rule": {
                                                                                        "effect": "ENABLE_INITIAL",
                                                                                        "condition": {
                                                                                            "type": "FUNC",
                                                                                            "ruleFunction": "initReadOnlyFields",
                                                                                            "value": [
                                                                                                {
                                                                                                    "$ref": "#/properties/State/properties/Code"
                                                                                                }
                                                                                            ]
                                                                                        }
                                                                                    },
                                                                                    "value": {
                                                                                        "$ref": "#/properties/Body/properties/file"
                                                                                    },
                                                                                    "width": 1
                                                                                }
                                                                            ]
                                                                        }
                                                                    ],
                                                                    "sectionLevel": 1
                                                                }
                                                            ],
                                                            "elementId": "tab-GeneralInfo",
                                                            "title": "GeneralInfo"
                                                        },
                                                        {
                                                            "type": "Tab",
                                                            "elements": [
                                                                {
                                                                    "type": "InlineView",
                                                                    "showContainerActions": false,
                                                                    "views": [
                                                                        {
                                                                            "viewReference": {
                                                                                "configurationCodeName": "CreditLifeImportLoadCoolOffSuccess",
                                                                                "configurationVersion": "1",
                                                                                "configurationConceptType": "SearchView"
                                                                            },
                                                                            "onViewInitialized": "initLoadedDataFromExcel",
                                                                            "viewType": "Search"
                                                                        }
                                                                    ],
                                                                    "value": {
                                                                        "$ref": "#"
                                                                    },
                                                                    "hide": false,
                                                                    "elementId": "InvestmentOptionPricesImportInlineView"
                                                                },
                                                                {
                                                                    "type": "Section",
                                                                    "label": "LoadDataErrors",
                                                                    "elements": [
                                                                        {
                                                                            "type": "InlineView",
                                                                            "showContainerActions": false,
                                                                            "views": [
                                                                                {
                                                                                    "viewReference": {
                                                                                        "configurationCodeName": "CreditLifeImportErrors",
                                                                                        "configurationVersion": "1",
                                                                                        "configurationConceptType": "SearchView"
                                                                                    },
                                                                                    "onViewInitialized": "initLoadedDataFromExcel",
                                                                                    "viewType": "Search"
                                                                                }
                                                                            ],
                                                                            "value": {
                                                                                "$ref": "#"
                                                                            },
                                                                            "hide": false,
                                                                            "elementId": "ImportLoadFailedInlineView"
                                                                        }
                                                                    ],
                                                                    "sectionLevel": 1
                                                                }
                                                            ],
                                                            "elementId": "tab-LoadedData",
                                                            "title": "LoadedData"
                                                        },
                                                        {
                                                            "type": "Tab",
                                                            "elements": [
                                                                {
                                                                    "type": "InlineView",
                                                                    "showContainerActions": false,
                                                                    "views": [
                                                                        {
                                                                            "viewReference": {
                                                                                "configurationCodeName": "CreditLifeImportSuccess",
                                                                                "configurationVersion": "1",
                                                                                "configurationConceptType": "SearchView"
                                                                            },
                                                                            "onViewInitialized": "initImpStatSucc",
                                                                            "viewType": "Search"
                                                                        }
                                                                    ],
                                                                    "value": {
                                                                        "$ref": "#"
                                                                    },
                                                                    "hide": false,
                                                                    "elementId": "FundsImportRecordStatusSuccessInlineView"
                                                                },
                                                                {
                                                                    "type": "InlineView",
                                                                    "showContainerActions": false,
                                                                    "views": [
                                                                        {
                                                                            "viewReference": {
                                                                                "configurationCodeName": "CreditLifeImportFailed",
                                                                                "configurationVersion": "1",
                                                                                "configurationConceptType": "SearchView"
                                                                            },
                                                                            "onViewInitialized": "initImpStatFail",
                                                                            "viewType": "Search"
                                                                        }
                                                                    ],
                                                                    "value": {
                                                                        "$ref": "#"
                                                                    },
                                                                    "hide": false,
                                                                    "elementId": "FundsImportRecordStatusFailedInlineView"
                                                                }
                                                            ],
                                                            "elementId": "tab-ImportedData",
                                                            "title": "ImportedData"
                                                        }
                                                    ]
                                                }
                                            ]
                                        }
                                    ],
                                    "numberOfColumns": 4,
                                    "width": 3
                                },
                                {
                                    "type": "SideContentLayout",
                                    "fixedPosition": true,
                                    "width": 1,
                                    "elements": [
                                        {
                                            "type": "GridLayout",
                                            "numberOfColumns": 2,
                                            "elements": [
                                                {
                                                    "type": "TabLayout",
                                                    "elements": [
                                                        {
                                                            "type": "Tab",
                                                            "icon": {
                                                                "name": "Clipboard-Check",
                                                                "size": "Large"
                                                            },
                                                            "elements": [
                                                                {
                                                                    "type": "Component",
                                                                    "name": "AuditInfo",
                                                                    "value": {
                                                                        "$ref": "#/properties/AuditInfo"
                                                                    }
                                                                }
                                                            ],
                                                            "elementId": "tab-Audit Info"
                                                        },
                                                        {
                                                            "type": "Tab",
                                                            "icon": {
                                                                "name": "Bell",
                                                                "size": "Large"
                                                            },
                                                            "tabType": "error",
                                                            "elements": [
                                                                {
                                                                    "type": "ValidationSummary",
                                                                    "elementId": "RequiredPropertiesValidations"
                                                                }
                                                            ],
                                                            "elementId": "tab-Notifications",
                                                            "additionalInfo": {
                                                                "display": "ValidationErrorsCount"
                                                            }
                                                        }
                                                    ]
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                },
                {
                    "type": "Component",
                    "name": "DocumentFooter",
                    "value": {
                        "$ref": "#"
                    }
                }
            ]
        }
    ]
}