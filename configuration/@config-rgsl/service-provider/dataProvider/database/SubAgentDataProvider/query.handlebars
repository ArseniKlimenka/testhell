SELECT
    sp.SERVICE_PROVIDER_CODE, 
    sp.BODY SERVICE_PROVIDER_BODY,
    sp.PARTY_CODE
FROM ORG.SERVICE_PROVIDER sp
JOIN ORG.APPLICATION_USER_CLAIM auc ON sp.PARTY_CODE = auc.VALUE AND auc.CLAIM_TYPE = N'PartyCode'
JOIN ORG.APPLICATION_USER au ON auc.APPLICATION_USER_ID = au.APPLICATION_USER_ID
WHERE 1=1
{{#if parameters.sadNumber}}
AND au.USERNAME = cast(@sadNumber as nvarchar(max))
{{/if}}