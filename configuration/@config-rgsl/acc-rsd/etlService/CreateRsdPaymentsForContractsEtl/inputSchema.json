{
    "$schema": "http://json-schema.org/draft-04/schema",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "rsdNumber",
        "transactionDate"
    ],
    "properties": {
        "rsdNumber": {
            "type": "string"
        },
        "contractNumbers": {
            "type": "array",
            "item": {
                "type": "string"
            }
        },
        "transactionDate": {
            "type": "string",
            "aiFormat": "date"
        },
        "skipStatusSwitch": {
            "type": "boolean"
        }
    }
}