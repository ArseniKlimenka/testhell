{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "criteria": {
            "type": "object",
            "additionalProperties": false,
            "required": [
                "from",
                "to"
            ],
            "properties": {
                "from": {
                    "type": "string",
                    "aiFormat": "date"
                },
                "to": {
                    "type": "string",
                    "aiFormat": "date"
                },
                "contractNumber": {
                    "type": "string"
                }
            }
        }
    }
}