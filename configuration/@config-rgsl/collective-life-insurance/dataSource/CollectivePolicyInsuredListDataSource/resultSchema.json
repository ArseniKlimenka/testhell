{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "id": {
            "type": "integer"
        },
        "contractNumber": {
            "type": "string"
        },
        "partyCode": {
            "type": "string"
        },
        "fullName": {
            "type": "string"
        },
        "surName": {
            "type": "string"
        },
        "firstName": {
            "type": "string"
        },
        "middleName": {
            "type": "string"
        },
        "birthDay": {
            "type": "string",
            "aiFormat": "date"
        },
        "gender": {
            "type": "string"
        },
        "mobile": {
            "type": "string"
        },
        "amount": {
            "type": "number"
        },
        "premium": {
            "type": "number"
        },
        "reinsurerCode": {
            "type": "string"
        },
        "reinsurerName": {
            "type": "string"
        },
        "reinsurerShare": {
            "type": "number"
        },
        "risks": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "startDate": {
                        "type": "string",
                        "aiFormat": "date"
                    },
                    "endDate": {
                        "type": "string",
                        "aiFormat": "date"
                    },
                    "risk": {
                        "type": "string"
                    },
                    "insAmount": {
                        "type": "number"
                    },
                    "insPremium": {
                        "type": "number"
                    },
                    "reinsRate": {
                        "type": "number"
                    },
                    "reinsPremium": {
                        "type": "number"
                    }
                }
            }
        }
    }
}

