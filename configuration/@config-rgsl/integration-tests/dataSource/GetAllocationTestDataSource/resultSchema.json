                        "type": "boolean"
                    },
                    "postAmount": {
                        "type": "number"
                    },
                    "isPosted": {
                        "type": "boolean"
                    },
                    "postingDate": {
                        "type": "string",
                        "aiFormat": "date"
                    }
                }
