{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "additionalProperties": false,
    "properties": {
        "allocationId": {
            "type": "integer"
        },
        "cancelled": {
            "type": "boolean"
        },
        "cancelledAllocationId": {
            "type": "integer"
        },
        "payAmount": {
            "type": "number"
        },
        "docAmount": {
            "type": "number"
        },
        "toleranceDocAmount": {
            "type": "number"
        },
        "payCurrencyCode": {
            "type": "string"
        },
        "documentNo": {
