{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Accounting certificate technical information",
    "description": "Data schema describing accounting technical information",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "policyWasFound": {
            "type": "boolean"
        },
        "availableInsuredPersons": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "personCode": {
                        "type": "string"
                    },
                    "partyType": {
                        "type": "string"
                    },
                    "fullName": {
                        "type": "string"
                    }
                }
            }
        },
        "isCopy": {
            "type": "boolean"
        },
        "copiedFrom": {
            "type": "string"
        }
    }
}