    "type": "object",
    "additionalProperties": false,
    "properties": {
        "employeePartyCode": {
            "type": "string"
        },
        "employeeLastName": {
            "type": "string"
        },
        "employeeFirstName": {
            "type": "string"
        },
        "employeeMiddleName": {
            "type": "string"
        },
        "certificateIssueDate": {
            "type": "string",
            "aiFormat": "date"
        },
        "taxCertificateFormat": {
            "type": "string"
        }
    }
}