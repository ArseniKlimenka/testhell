{
    "$schema": "http://json-schema.org/draft-04/schema",
    "description": "Input schema for policy enrichments service.",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "body": {
            "type": "object",
            "additionalProperties": true,
            "description": "Accounting certificate body from request"
        },
        "configurationCodeName": {
            "type": "string",
            "description": "Accounting certificate configuration code name"
        },
        "configurationVersion": {
            "type": "string",
            "description": "Accounting certificate configuration version"
        },
        "isContractEnrichOnly": {
            "type": "boolean",
            "description": "Accounting certificate document need enrich only policy data"
        },
        "currentCertificateNumber": {
            "type": "string",
            "description": "Accounting certificate document current number"
        },
        "incomingSource": {
            "type": "integer",
            "description": "Accounting certificate incoming source"
        },
        "sequenceNumber": {
            "type": "integer",
            "description": "Accounting certificate sequence number"
        }
    }
}
