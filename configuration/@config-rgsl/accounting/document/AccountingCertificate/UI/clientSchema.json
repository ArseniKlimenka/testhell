{
    "$schema": "http://json-schema.org/draft-04/schema",
    "type": "object",
    "additionalProperties": true,
    "properties": {
        "canAddAttachmentWithType": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "attachmentType": {
                    "type": "string"
                },
                "allowed": {
                    "type": "boolean"
                }
            }
        },
        "attachmentsCommentMap": {
            "type": "array",
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "attachmentType": {
                        "type": "string"
                    },
                    "customName": {
                        "type": "string"
                    }
                }
            }
        },
        "isContractEnrichOnly": {
            "type": "boolean"
        }
    }
}