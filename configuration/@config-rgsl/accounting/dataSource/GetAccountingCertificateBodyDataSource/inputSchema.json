{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "Accounting year data source input schema",
    "type": "object",
    "properties": {
        "criteria": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "accountingCertificateNumber": {
                    "type": "string"
                },
                "accountingCertificateState": {
                    "$ref": "component:AccountingCertificateState"
                },
                "accountingCertificateStates": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "accountingCertificateSeqNumber": {
                    "type": "string"
                },
                "originalDocumentNumber": {
                    "type": "string"
                },
                "contractNumber": {
                    "type": "string"
                },
                "holderPartyCode": {
                    "type": "string"
                },
                "insuredPartyCode": {
                    "type": "string"
                },
                "applicantFullName": {
                    "type": "string"
                },
                "requestDate": {
                    "type": "string",
                    "aiFormat": "date"
                },
                "requestDateFrom": {
                    "type": "string",
                    "aiFormat": "date"
                },
                "requestDateTo": {
                    "type": "string",
                    "aiFormat": "date"
                },
                "accountingYear": {
                    "type": "string"
                },
                "correctionNumber": {
                    "type": "integer"
                },
                "amountOfPremiumPaid": {
                    "type": "number"
                },
                "certificateIssueDate": {
                    "type": "string",
                    "aiFormat": "date"
                },
                "accountingCertificateNumbers": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            }
        }
    }
}