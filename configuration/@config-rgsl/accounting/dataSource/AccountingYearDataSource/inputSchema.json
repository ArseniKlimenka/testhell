{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "Accounting year data source input schema",
    "type": "object",
    "properties": {
        "criteria": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "year": {
                    "type": "string"
                },
                "years": {
                    "type": "array",
                    "items": {
                        "type":"string"
                    }
                },
                "fromYear": {
                    "type": "string"
                },
                "toYear": {
                    "type": "string"
                }
            }
        }
    }
}
