{
    "$schema": "http://json-schema.org/draft-04/schema",
    "description": "Export excel accounting certificate data schema",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "accountingCertificateNumber": {
                "type": "string"
            },
            "accountingCertificateState": {
                "type": "string"
            },
            "accountingCertificateStateDescription": {
                "type": "string"
            },
            "accountingCertificateSeqNumber": {
                "type": "string"
            },
            "originalDocumentNumber": {
                "type": "string"
            },
            "contractNumber": {
                "type": "string"
            },
            "contractCodeName": {
                "type": "string"
            },
            "applicantFullName": {
                "type": "string"
            },
            "requestDate": {
                "type": "string",
                "aiFormat": "date"
            },
            "accountingYear": {
                "type": "string"
            },
            "correctionNumber": {
                "type": "integer"
            },
            "amountOfPremiumPaid": {
                "type": "number"
            },
            "certificateIssueDate": {
                "type": "string",
                "aiFormat": "date"
            },
            "transitionCommitor": {
                "type": "string"
            },
            "hasAttachment": {
                "type": "boolean"
            }
        }
    }
}