{
    "$schema": "http://json-schema.org/draft-04/schema",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "importDocumentId": {
            "type": "string",
            "description": "Id of the import document"
        },
        "importDocumentNumber": {
            "type": "string",
            "description": "Business number of the import document"
        },
        "importDocumentOrganisation": {
            "type": "object",
            "properties": {
                "id":{
                    "type":"string"
                },
                "code":{
                    "type":"string"
                },
                "name":{
                    "type":"string"
                },
                "parentId":{
                    "type":"string"
                },
                "unitCodes":{
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "officeCodes":{
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            }         
        }
    }
}