{
    "$schema": "http://json-schema.org/draft-04/schema",
    "type": "object",
    "additionalProperties": false,
    "aiValidation": {
        "rule": "validateEmployee"
    },
    "properties": {
        "employeeParty": {
            "$ref": "component:EmployeeParty"
        },
        "position": {
            "type": "string",
            "aiTitle": "position"
        },
        "tabNumber": {
            "type": "string",
            "aiTitle": "tabNumber"
        },
        "orgUnitName": {
            "type": "string"
        },
        "orgUnitCode": {
            "type": "string"
        },
        "visibilityType": {
            "aiTitle": "visibilityType",
            "$ref": "component:EnumVisibilityType"
        },
        "isPersonalManager": {
            "type": "boolean",
            "aiTitle": "isPersonalManager"
        },
        "sadNumber": {
            "type": "integer",
            "aiTitle": "sadNumber"
        },
        "physicalPersonID": {
            "type": "string",
            "aiTitle": "physicalPersonID"
        },
        "sadBeginDate": {
            "type": "string",
            "aiFormat": "date",
            "aiTitle": "sadBeginDate"
        },
        "sadEndDate": {
            "type": "string",
            "aiFormat": "date",
            "aiTitle": "sadEndDate"
        },
        "lnrNumber": {
            "type": "integer",
            "aiTitle": "lnrNumber"
        },
        "positionKFN": {
            "type": "string",
            "aiTitle": "positionKFN"
        },
        "sadNumber1": {
            "type": "string",
            "aiTitle": "sadNumber1"
        },
        "sadNumber2": {
            "type": "string",
            "aiTitle": "sadNumber2"
        },
        "sadNumberMAG": {
            "type": "string",
            "aiTitle": "sadNumberMAG"
        },
        "sadNumberNSO": {
            "type": "string",
            "aiTitle": "sadNumberNSO"
        },
        "isSASAgent": {
            "type": "boolean",
            "aiTitle": "isSASAgent"
        },
        "level": {
            "type": "string",
            "aiTitle": "level",
            "enum": [
                "level1",
                "level2",
                "level3",
                "level4"
            ]
        },
        "sadFinal": {
            "type": "string",
            "aiTitle": "sadFinal"
        },
        "SKK": {
            "type": "integer",
            "aiTitle": "SKK"
        },
        "receiveType": {
            "type": "integer",
            "aiTitle": "receiveType"
        },
        "order": {
            "type": "string",
            "aiTitle": "order",
            "enum": [
                "6000461",
                "6000260"
            ]
        },
        "sapAD": {
            "type": "string",
            "aiTitle": "sapAD"
        },
        "salesChannel": {
            "type": "string",
            "aiTitle": "salesChannel"
        },
        "branch": {
            "type": "object",
            "aiTitle": "branch",
            "additionalProperties": false,
            "properties": {
                "branchId": {
                    "type": "string"
                },
                "branchName": {
                    "type": "string",
                    "aiTitle": "branchName"
                },
                "branchKSP": {
                    "type": "string",
                    "aiTitle": "branchKSP"
                },
                "branchRegion": {
                    "type": "string",
                    "aiTitle": "branchRegion"
                }
            }
        },
        "department": {
            "type": "string",
            "aiTitle": "department"
        },
        "actualEmail": {
            "type": "string",
            "aiTitle": "actualEmail"
        }
    }
}