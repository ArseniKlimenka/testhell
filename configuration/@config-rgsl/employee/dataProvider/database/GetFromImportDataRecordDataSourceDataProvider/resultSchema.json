{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "Entries loaded into staging table for an import document.",
    "type": "object",
    "properties": {
        "IMPORT_DOCUMENT_ID": {
            "type": "string",
            "description": "Import document ID.",
            "aiFormat": "guid"
        },
        "SOURCE_ID": {
            "type": "string",
            "description": "Identifier of the import source."
        },
        "RECORD_KEY": {
            "type": "string",
            "description": "Record key."
        },
        "DATA": {
            "type": "object",
            "description": "Data blob.",
            "additionalProperties": true
        },
        "EXCEL_ROW_NUMBER": {
            "type": "integer",
            "description": "Excel row number"
        }
    },
    "additionalProperties": false
}