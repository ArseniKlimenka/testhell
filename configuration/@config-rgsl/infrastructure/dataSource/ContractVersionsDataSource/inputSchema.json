{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "properties": {
        "criteria": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "originalDocumentId": {
                    "type": "string"
                },
                "contractNumber": {
                    "type": "string"
                },
                "contractNumberStrict": {
                    "type": "string"
                },
                "contractNumbersStrict": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "seqNumber": {
                    "type": "integer"
                },
                "seqNumberMax": {
                    "type": "boolean"
                },
                "policyOnly": {
                    "type": "boolean"
                },
                "versionState": {
                    "type": "string"
                },
                "versionStateWithNull": {
                    "type": "string"
                },
                "stateId": {
                    "type": "integer"
                }
            }
        },
        "sort": {
            "type": "array",
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "descending": {
                        "type": "boolean"
                    },
                    "fieldName": {
                        "type": "string"
                    }
                }
            }
        }
    }
}