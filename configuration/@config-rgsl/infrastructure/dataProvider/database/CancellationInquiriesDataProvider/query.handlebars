SELECT inqDoc.UNIVERSAL_DOCUMENT_NUMBER,
	   inqDoc.SYS_CREATED_ON,
	   inqDoc.SYS_UPDATED_ON,
	   st.CODE_NAME AS STATE,
	   inqSat.DEPARTMENT_CODE
FROM 
BFX.UNIVERSAL_DOCUMENT inqDoc
JOIN CFG.PROCESS_STATE st ON st.PROCESS_STATE_ID = inqDoc.STATE_ID
JOIN PAS_IMPL.INQUIRY_HUB inqHub ON inqHub.INQUIRY_NUMBER = inqDoc.UNIVERSAL_DOCUMENT_NUMBER
JOIN PAS_IMPL.INQUIRY_SAT_LATEST inqSat ON inqSat.INQUIRY_HKEY = inqHub.INQUIRY_HKEY
JOIN PAS_IMPL.CNL_INQUIRY_LINK inqLink ON inqLink.INQUIRY_HKEY = inqHub.INQUIRY_HKEY
JOIN PAS_IMPL.AMENDMENT_HUB amndHub ON amndHub.AMENDMENT_HKEY = inqLink.AMENDMENT_HKEY
WHERE amndHub.AMENDMENT_NUMBER = @cancellationNumber