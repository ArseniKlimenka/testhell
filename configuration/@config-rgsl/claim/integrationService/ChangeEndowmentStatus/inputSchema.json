{
    "$schema": "http://json-schema.org/draft-04/schema",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "transitionName",
        "endowments"
    ],
    "properties": {
        "transitionName": {
            "type": "string"
        },
        "endowments": {
            "type": "array",
            "items": {
                "type": "object",
                "additionalProperties": false,
                "required": [
                    "number",
                    "state"
                ],
                "properties": {
                    "number": {
                        "type": "string"
                    },
                    "state": {
                        "type": "string"
                    }
                }
            }
        }
    }
}