{
    "$schema": "http://json-schema.org/draft-04/schema",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "existingPaymentOrders": {
            "type": "array",
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "paymentOrderNumber": {
                        "type": "string"
                    },
                    "beneficiaryCode": {
                        "type": "string"
                    }
                }
            }
        },
        "relatedClaims": {
            "type": "array",
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "claimNumber": {
                        "type": "string"
                    },
                    "contractNumber": {
                        "type": "string"
                    },
                    "claimState": {
                        "type": "string"
                    },
                    "paymentOrderNumber": {
                        "type": "string"
                    },
                    "paymentOrderState": {
                        "type": "string"
                    },
                    "insuredEventDate": {
                        "type": "string"
                    }
                }
            }
        }
    }
}