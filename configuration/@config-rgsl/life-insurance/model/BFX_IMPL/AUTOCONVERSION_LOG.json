{
    "$schema": "http://json-schema.org/draft-04/schema",
    "type": "object",
    "properties": {
        "RECORD_ID": {
            "type": "number",
            "aiType": "guid",
            "aiPrimaryKey": true,
            "aiAutogenerated": true
        },
        "EXECUTED_ON": {
            "type": "string",
            "aiType": "datetime2"
        },
        "CONTRACT_NUMBER": {
            "type": "string",
            "aiType": "nvarchar(128)"
        },

        "ERROR_MESSAGE": {
            "type": "string",
            "aiType": "nvarchar(max)"
        },
        "ERROR_CODE": {
            "type": "string",
            "aiType": "nvarchar(512)"
        },
        "ERROR_MESSAGE_DESCRIPTION": {
            "type": "string",
            "aiType": "nvarchar(max)"
        }
    }
}