{
    "$schema": "http://json-schema.org/draft-04/schema",
    "type": "object",
    "title": "Insurance rules configuration",
    "properties": {
        "CONF_VERSION": {
            "title": "Excel loaded version",
            "type": "number",
            "aiType": "int",
            "aiPrimaryKey": true
        },
        "EXCEL_ROW_NUMBER": {
            "type": "integer",
            "aiType": "int",
            "aiPrimaryKey": true
        },
        "RULE_NUMBER": {
            "type": "integer",
            "aiType": "int"
        },
        "RULE_CODE": {
            "type": "string",
            "aiType": "nvarchar(255)"
        },
        "RULE_DESCRIPTION": {
            "type": "string",
            "aiType": "nvarchar(max)"
        },
        "RULE_DATE": {
            "type": "string",
            "aiType": "date"
        },
        "RULE_LINK": {
            "type": "string",
            "aiType": "nvarchar(max)"
        },
        "IMPORT_DOCUMENT_ID": {
            "type": "number",
            "aiType": "guid"
        },
        "LOADED_BY": {
            "type": "string",
            "aiType": "nvarchar(max)"
        },
        "LOAD_DATE": {
            "type": "string",
            "aiType": "datetime2"
        }
