{
    "$schema": "http://json-schema.org/draft-04/schema",
    "description": "Result schema for StrategyInstrFileLoader data source.",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "$recordKey": {
            "type": "string"
        },
        "data": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "excelRowNumber": {
                    "type": "integer"
                },
                "productCode": {
                    "type": "string"
                },
                "strategyCode": {
                    "type": "string"
                },
                "issueDateFrom": {
                    "type": "string",
                    "aiFormat": "date"
                },
                "issueDateTo": {
                    "type": "string",
                    "aiFormat": "date"
                },
                "issueDateStr": {
                    "type": "string"
                },
                "productDescription": {
                    "type": "string"
                },
                "strategyDescriptionFull": {
                    "type": "string"
                },
                "purchaseDate": {
                    "type": "string",
                    "aiFormat": "date"
                },
                "dischargeDate": {
                    "type": "string",
                    "aiFormat": "date"
                },
                "didBeginDate": {
                    "type": "string",
                    "aiFormat": "date"
                },
                "didEndDate": {
                    "type": "string",
                    "aiFormat": "date"
                },
                "couponPeriods": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "beginDate": {
                                "type": "string",
                                "aiFormat": "date"
                            },
                            "endDate": {
                                "type": "string",
                                "aiFormat": "date"
                            }
                        }
                    }
                },
                "windowStartDate": {
                    "type": "string",
                    "aiFormat": "date"
                },
                "windowEndDate": {
                    "type": "string",
                    "aiFormat": "date"
                }
            }
        }
    }
}
