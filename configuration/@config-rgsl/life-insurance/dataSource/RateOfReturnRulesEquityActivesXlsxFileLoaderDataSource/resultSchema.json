{
    "$schema": "http://json-schema.org/draft-04/schema",
    "description": "Result schema for ProductConfigurationFileLoader data source.",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "$recordKey": {
            "type": "string"
        },
        "data": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "excelRowNumber": {
                    "type": "integer"
                },
                "productCode": {
                    "type": "string"
                },
                "insuranceTerms": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "issueDateFrom": {
                    "type": "string",
                    "aiFormat": "date"
                },
                "issueDateTo": {
                    "type": "string",
                    "aiFormat": "date"
                },
                "issueDateStr": {
                    "type": "string"
                },
                "manualRate": {
                    "type": "number"
                },
                "investmentFrequency": {
                    "type": "number"
                },
                "mf": {
                    "type": "number"
                },
                "costsOpenContracts": {
                    "type": "number"
                },
                "commWithdrawalFunds": {
                    "type": "object",
                    "additionalProperties": true
                },
                "rko": {
                    "type": "number"
                },
                "isStandardContractConditions": {
                    "type": "boolean"
                },
                "isCoordinationUDRequired": {
                    "type": "boolean"
                },
                "importDocumentId": {
                    "type": "string"
                },
                "version": {
                    "type": "integer"
                }
            }
        }
    }
}
