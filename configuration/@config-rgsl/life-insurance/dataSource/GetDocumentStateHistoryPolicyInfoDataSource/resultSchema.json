{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "Contracts returned by data provider.",
    "properties": {
        "contractNumber": {
            "type": "string",
            "description": "Policy number."
        },
        "holderName": {
            "type": "string",
            "description": "Policy holder name."
        },
        "stateCode": {
            "type": "string",
            "description": "Document state code."
        },
        "state": {
            "type": "string",
            "description": "Document state name."
        },
        "transitionCode": {
            "type": "string",
            "description": "Document transition code."
        },
        "transition": {
            "type": "string",
            "description": "Document transition name."
        },
        "changedOn": {
            "type": "string",
            "aiFormat": "date-time",
            "description": "When transition was done."
        }
    }
}
