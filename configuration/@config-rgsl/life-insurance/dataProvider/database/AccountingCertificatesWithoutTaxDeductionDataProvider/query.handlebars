SELECT ud.UNIVERSAL_VERSIONED_DOCUMENT_ID,
      ud.UNIVERSAL_VERSIONED_DOCUMENT_NUMBER,
      csl.ACCOUNTING_YEAR,
      csl.CORRECTION_NUMBER,
      csl.CONTRACT_NUMBER
FROM [ACC_IMPL].[CRT_SAT_LATEST] csl
JOIN [BFX].[UNIVERSAL_VERSIONED_DOCUMENT] ud on ud.UNIVERSAL_VERSIONED_DOCUMENT_NUMBER = csl.ORIGINAL_DOCUMENT_NUMBER
join cfg.PROCESS_STATE st on st.PROCESS_STATE_ID = ud.STATE_ID
WHERE st.CODE_NAME = 'Issued' 
AND csl.ORIGINAL_DOCUMENT_NUMBER NOT IN (
    SELECT ud.UNIVERSAL_VERSIONED_DOCUMENT_NUMBER
    FROM [BFX].[ATTACHMENT_RELATED_ENTITY] are
    JOIN [BFX].[UNIVERSAL_VERSIONED_DOCUMENT] ud ON are.ENTITY_REF_ID = ud.UNIVERSAL_VERSIONED_DOCUMENT_ID
    JOIN [BFX].ATTACHMENT a ON are.ATTACHMENT_ID = a.ATTACHMENT_ID
    WHERE a.ATTACHMENT_TYPE = 'taxDeductionCertificate'
    AND a.UPLOAD_STATUS = 2
  )