select
  plh.CONTRACT_NUMBER,
  art.CODE_NAME,
  art.PUBLISHED_VERSION,
  pls.ISSUE_DATE,
  pls.PRODUCT_CODE
from
  PAS_IMPL.POLICY_HUB plh,
  PAS_IMPL.POLICY_SAT_LATEST pls,
  PAS.CONTRACT agr,
  CFX.PUBLISHED_ARTIFACT art
where plh.POLICY_HKEY = pls.POLICY_HKEY
  and plh.CONTRACT_NUMBER = agr.CONTRACT_NUMBER
  and agr.PUBLISHED_ARTIFACT_ID = art.PUBLISHED_ARTIFACT_ID