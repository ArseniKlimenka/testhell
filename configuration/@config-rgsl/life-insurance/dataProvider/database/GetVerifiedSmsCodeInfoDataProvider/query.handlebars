SELECT SECURITY_CODE,
		   REFERENCE_NUMBER,
		   NOTIFICATION_DATE
	FROM
	(SELECT SECURITY_CODE,
			REFERENCE_NUMBER,
			NOTIFICATION_DATE,
			RANK() OVER (PARTITION BY REFERENCE_NUMBER ORDER BY NOTIFICATION_DATE DESC) AS RANK
	 FROM 
	 BFX_IMPL.SMS_SECURITY_NOTIFICATION
	 WHERE REFERENCE_NUMBER = @referenceNo
	 AND IS_VERIFIED = 1) sq
WHERE sq.RANK = 1