      ,PRODUCT_DESCRIPTION
      ,PRODUCT_GROUP_CODE
      ,PAYMENT_FREQUENCY
      ,INSURANCE_TERMS
      ,INSURANCE_TERMS_MONTHS
      ,IS_WHOLE_LIFE
      ,INSURED_IS_POLICY_HOLDER
      ,HOLDER_AGE_ON_START_DATE_MIN
      ,HOLDER_AGE_ON_START_DATE_MAX
      ,HOLDER_AGE_ON_START_DATE_MAX_MA
      ,HOLDER_AGE_ON_END_DATE_MAX
      ,INSURED_AGE_ON_START_DATE_MIN
      ,INSURED_AGE_ON_START_DATE_MAX
      ,INSURED_AGE_ON_START_DATE_MA
      ,INSURED_AGE_ON_END_DATE_MAX
      ,INSURED_AGE_ON_END_DATE_MAX_MA
      ,STRATEGY
      ,ADDITIONAL_SERVICES
      ,GIFT_SERVICES
      ,GIFT_SERVICES_PREMIUM
      ,PREFIX_OLD
      ,PREFIX
      ,PREFIX_BY_STRATEGY_OLD
      ,PREFIX_BY_STRATEGY
      ,FIXED_PREMIUMS
      ,FIXED_INSURED_SUMS
      ,SET_FIRST_FIXED_INSURED_SUM
      ,DISABLE_RISK_INSURED_SUM
      ,ALLOW_CALC_FROM_PREMIUM
      ,ALLOW_CALC_FROM_INSURED_SUM
      ,CAN_HAVE_ADDITIONAL_RISKS
      ,MIN_RISK_INSURED_SUM
      ,MAX_RISK_INSURED_SUM
      ,MIN_PREMIUM
      ,MAX_PREMIUM
      ,MAX_PREMIUM_MA
      ,MAX_INSURED_SUM_MAIN_RISK
      ,MAX_INSURED_SUM_MAIN_RISK_MA
      ,IS_PAID_UP_AVAILABLE
      ,RULE_CODE
      ,DAYS_BETWEEN_ISSUE_AND_START
      ,PAY_PERIOD_DAYS
      ,GRACE_PERIOD_DAYS
      ,COOL_OFF_PERIOD_DAYS
      ,PARTNER_BUSINESS_CODE
      ,RISK_PACKAGES
      ,USE_THREE_PAYMENTS
      ,ACTIVE_FROM
      ,ACTIVE_TO
      ,PAPER_TYPES
      ,APPLICATION_PRINTOUT
      ,POLICY_PRINTOUT
      ,CASH_BACK_COEFFICIENT
      ,SHOW_FIN_KNOW_QUEST
      ,IS_REINVEST_AVAILABLE
      ,IS_REINVEST_FIELDS_AVAILABLE
      ,INVOICE_ON_ACTIVATION_IF_REINVEST
      ,DAYS_BETWEEN_ISSUE_AND_START_REINVEST
      ,PAY_PERIOD_DAYS_REINVEST
      ,CREDIT_PROGRAMS
      ,AVAILABLE_CURRENCIES
      ,IS_MIGRATED
      ,GUARANTEED_INCOME
      ,AVAILABLE_PAYMENT_FREQUENCY
      ,POLICY_HOLDER_TYPE
      ,CARD_TYPE
      ,CUMULATION_PRODUCT_GROUP
      ,INSURANCE_TERMS_DAYS
      ,IS_PRODUCT_LINKED_TO_ASSET
      ,DID_TYPE
      ,NUM_OF_WORKING_DAYS_TO_INVEST
      ,MF
      ,COOL_OFF_DID_RATE
      ,CONSENT_TO_DATA_TRANSFERING_FNS
      ,IMPORT_DOCUMENT_ID
      ,CONF_VERSION
      ,LOADED_BY
      ,LOAD_DATE
FROM BFX_IMPL.PRODUCT_CONF pc
WHERE 1 = 1
{{#if parameters.productCode}}
	AND pc.PRODUCT_CODE = @productCode
{{/if}}
{{#if parameters.issueDate}}
	AND @issueDate >= pc.ISSUE_DATE_FROM AND @issueDate <= pc.ISSUE_DATE_TO
{{/if}}
{{#if parameters.issueDateFrom}}
	AND pc.ISSUE_DATE_FROM >= @issueDateFrom
{{/if}}
{{#if parameters.issueDateTo}}
	AND @issueDateTo <= pc.ISSUE_DATE_TO
{{/if}}
{{#if parameters.importDocumentId}}
	AND pc.IMPORT_DOCUMENT_ID = @importDocumentId
{{/if}}
{{#if parameters.maxVersion}}
	AND pc.CONF_VERSION = (SELECT MAX(CONF_VERSION) MAX_VERSION FROM BFX_IMPL.PRODUCT_CONF)
{{/if}}
{{#if parameters.partnerBusinessCode}}
	AND pc.PARTNER_BUSINESS_CODE = @partnerBusinessCode
{{/if}}
{{#if parameters.version}}
	AND pc.CONF_VERSION = @version
{{/if}}
{{#if parameters.productDescription}}
	AND pc.PRODUCT_DESCRIPTION LIKE '%' + @productDescription + '%'
{{/if}}