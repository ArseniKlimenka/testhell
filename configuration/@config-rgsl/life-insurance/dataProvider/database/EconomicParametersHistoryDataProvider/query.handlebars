SELECT
	 aps.PRODUCT_CONF_NUMBER
    ,aps.RULE_NUM
    ,aps.SEGMENT
    ,aps.ISIN
    ,aps.RKO
    ,aps.MOTIVATION_FROM_MARGIN
    ,aps.MOTIVATION_FROM_PRODUCT_ECONOMIC
    ,aps.SK_MARGIN
    ,aps.FUNDING_RATE_SWAPS
    ,aps.LAPS
    ,aps.HEDGE
    ,aps.CLIENT_ID
    ,aps.SHARE_RF
    ,aps.SHARE_GF
    ,aps.RVD
    ,aps.FUNDING_VER_SUB_FUND_ID
    ,aps.MEMORANDUM_PK_DATE
    ,aps.PK_NUMBER
    ,aps.ANALYTICAL_ADJUSTMENT
    ,aps.EXPECTED_RETURN_PERCENT_AK
    ,aps.INSURANCE
    ,aps.RISK_TRANSFER_PRODUCT
    ,aps.COMMENTS
    ,aps.IS_MANUAL_CORRECTION
    ,aps.TRIGGERED_BY_USERNAME
	,aps.LOAD_DATE
FROM BFX.UNIVERSAL_MASTER_ENTITY ume
JOIN BFX_IMPL.ADDITIONAL_PARAMETERS_HUB aph ON aph.DOCUMENT_NUMBER = ume.UNIVERSAL_MASTER_ENTITY_CODE
JOIN BFX_IMPL.ADDITIONAL_PARAMETERS_SAT aps ON aps.ADDITIONAL_PARAMETERS_HKEY = aph.ADDITIONAL_PARAMETERS_HKEY
WHERE UNIVERSAL_MASTER_ENTITY_CODE = @universalMasterEntityCode