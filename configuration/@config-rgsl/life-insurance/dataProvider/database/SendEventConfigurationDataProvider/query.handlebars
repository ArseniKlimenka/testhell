select
  cnf.SEND_EVENT_CONFIGURATION_ID,
  cnf.SEND_EVENT_TYPE_ID,
  etp.SEND_EVENT_TYPE_NAME,
  cnf.SUBSCRIBER,
  cnf.ENTITY_CODE,
  cnf.EVENT_CODE,
  cnf.BEGIN_DATE,
  cnf.END_DATE,
  cnf.API_SENDER,
  cnf.IS_RECORD_DISABLED
from
  BFX_IMPL.SEND_EVENT_CONFIGURATION cnf,
  BFX_IMPL.SEND_EVENT_TYPE etp
where cnf.SEND_EVENT_TYPE_ID = etp.SEND_EVENT_TYPE_ID
  and etp.SEND_EVENT_TYPE_NAME = @eventType