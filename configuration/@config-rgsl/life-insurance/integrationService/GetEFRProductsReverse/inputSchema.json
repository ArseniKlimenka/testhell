{
    "$schema": "http://json-schema.org/draft-04/schema",
    "description": "Input schema for EFR products reverse.",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "contracts"
    ],
    "properties": {
        "contracts": {
            "type": "array",
            "items": {
                "type": "object",
                "additionalProperties": false,
                "required": [
                    "number",
                    "productDescription",
                    "productCode",
                    "productGroupDescription"
                ],
                "properties": {
                    "number": {
                        "type": "string",
                        "description": "Contract number"
                    },
                    "productDescription": {
                        "type": "string",
                        "description": "Product description"
                    },
                    "investmentStrategyDescription": {
                        "type": "string",
                        "description": "Investment strategy description"
                    },
                    "productCode": {
                        "type": "string",
                        "description": "Product code"
                    },
                    "productGroupDescription": {
                        "type": "string",
                        "description": "Product group description"
                    }
                }
            }
        }
    }
}