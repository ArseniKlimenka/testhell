{
    "$schema": "http://json-schema.org/draft-04/schema",
    "description": "Response schema for correct EFR products reversed response.",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "id": {
            "type": "string",
            "description": "Generated id for response."
        },
        "data": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "contracts": {
                    "type": "array",
                    "description": "Array of risk response objects",
                    "items": {
                        "type": "object",
                        "number": {
                            "type": "string",
                            "description": "Contract number"
                        },
                        "productDescription": {
                            "type": "string",
                            "description": "Product description"
                        },
                        "investmentStrategyDescription": {
                            "type": "string",
                            "description": "Investment strategy description"
                        },
                        "productCode": {
                            "type": "string",
                            "description": "Product code"
                        },
                        "productGroupDescription": {
                            "type": "string",
                            "description": "Product group description"
                        },
                        "efrProductGroupCode": {
                            "type": "string",
                            "description": "EFR product group code"
                        },
                        "efrProductDescription": {
                            "type": "string",
                            "description": "EFR product description"
                        },
                        "efrInvestmentStrategyDescription": {
                            "type": "string",
                            "description": "EFR investment strategy description"
                        },
                        "efrProductCode": {
                            "type": "string",
                            "description": "EFR product code"
                        },
                        "efrProductGroupDescription": {
                            "type": "string",
                            "description": "EFR product group description"
                        }
                    }
                }
            }
        }
    }
}
