{
    "$schema": "http://json-schema.org/draft-04/schema",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "contractNumber"
    ],
    "properties": {
        "contractNumber": {
            "type":"string"
        },
        "initiator": {
            "type":"object",
            "additionalProperties": false,
            "properties": {
                "userId": {
                    "type": "string"
                },
                "userName": {
                    "type": "string"
                },
                "partyFullName": {
                    "type": "string"
                },
                "partyCode": {
                    "type": "string"
                },
                "employeeCode": {
                    "type": "string"
                },
                "organisationUnitCode": {
                    "type": "string"
                },
                "organisationUnitName": {
                    "type": "string"
                }
            }
        },
        "orgUnit": {
            "type":"object",
            "additionalProperties": false,
            "properties": {
                "code": {
                    "type": "string"
                },
                "description": {
                    "type": "string"
                }
            }
        },
        "curator": {
            "type": "string"
        },
        "productConfigurationVersion": {
            "type": "integer"
        },
        "productCode": {
            "type": "string"
        },
        "issueDate": {
            "type": "string"
        },
        "modificationType":{
            "type": "string"
        }
    }
}