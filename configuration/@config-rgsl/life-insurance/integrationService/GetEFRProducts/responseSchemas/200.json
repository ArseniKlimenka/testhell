{
    "$schema": "http://json-schema.org/draft-04/schema",
    "description": "Response schema for correct EFR products response.",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "id": {
            "type": "string",
            "description": "Generated id for response."
        },
        "data": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "covers": {
                    "type": "array",
                    "description": "Array of cover objects",
                    "items": {
                        "type": "object",
                        "cover": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                },
                "risks": {
                    "type": "array",
                    "description": "Array of risk objects",
                    "items": {
                        "type": "object",
                        "risk": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                },
                "products": {
                    "type": "array",
                    "description": "Array of product objects",
                    "items": {
                        "type": "object",
                        "product": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                },
                "surveys": {
                    "type": "array",
                    "description": "Array of survey objects",
                    "items": {
                        "type": "object",
                        "survey": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                },
                "variants": {
                    "type": "array",
                    "description": "Array of variant objects",
                    "items": {
                        "type": "object",
                        "variant": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                },
                "ageLimits": {
                    "type": "array",
                    "description": "Array of limit objects",
                    "items": {
                        "type": "object",
                        "ageLimit": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                },
                "risksInPackages": {
                    "type": "array",
                    "description": "Array of risk in package objects",
                    "items": {
                        "type": "object",
                        "riskInPackage": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                },
                "services": {
                    "type": "array",
                    "description": "Array of services",
                    "items": {
                        "type": "object",
                        "service": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                },
                "strategies": {
                    "type": "array",
                    "description": "Array of strategies",
                    "items": {
                        "type": "object",
                        "strategy": {
                            "type": "object",
                            "additionalProperties": true
                        }
                    }
                }
            }
        }
    }
}

