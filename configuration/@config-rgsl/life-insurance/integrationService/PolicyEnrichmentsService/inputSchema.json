{
    "$schema": "http://json-schema.org/draft-04/schema",
    "description": "Input schema for policy enrichments service.",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "body": {
            "type": "object",
            "additionalProperties": true,
            "description": "Quote or policy body from request"
        },
        "originalDocumentNumber": {
            "type": "string",
            "description": "Quote or policy original document number"
        },
        "documentNumber": {
            "type": "string",
            "description": "Quote or policy document number"
        },
        "configurationCodeName": {
            "type": "string",
            "description": "Quote or policy configuration code name"
        },
        "configurationVersion": {
            "type": "string",
            "description": "Quote or policy configuration version"
        },
        "contractType": {
            "type": "string",
            "description": "Quote or policy configuration type"
        }
    }
}
