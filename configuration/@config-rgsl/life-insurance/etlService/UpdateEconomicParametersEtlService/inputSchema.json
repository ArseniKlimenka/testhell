{
    "$schema": "http://json-schema.org/draft-04/schema",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "productCode"
    ],
    "properties": {
        "productConfigurationNumber": {
            "type":"string"
        },
        "productCode": {
            "type":"string"
        },
        "selectedRules": {
            "type":"array",
            "items": {
                "type": "number"
            }
        },
        "contractNumbers": {
            "type":"array",
            "items": {
                "type": "string"
            }
        }
    }
}