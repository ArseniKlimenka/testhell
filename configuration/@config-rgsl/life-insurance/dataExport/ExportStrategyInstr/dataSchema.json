{
    "$schema": "http://json-schema.org/draft-04/schema",
    "description": "Export strategy instruments",
    "type": "array",
    "items": {
        "type": "object",
        "properties": {
            "ruleNumber": {
                "type": "integer"
            },
            "productCode": {
                "type": "string"
            },
            "strategyCode": {
                "type": "string"
            },
            "issueDate": {
                "type": "string"
            },
            "productDescription": {
                "type": "string"
            },
            "strategyDescriptionFull": {
                "type": "string"
            },
            "purchaseDate": {
                "type": "string",
                "aiFormat": "date"
            },
            "dischargeDate": {
                "type": "string",
                "aiFormat": "date"
            },
            "didBeginDate": {
                "type": "string",
                "aiFormat": "date"
            },
            "didEndDate": {
                "type": "string",
                "aiFormat": "date"
            },
            "couponPeriods": {
                "type": "string"
            },
            "windowStartDate": {
                "type": "string",
                "aiFormat": "date"
            },
            "windowEndDate": {
                "type": "string",
                "aiFormat": "date"
            }
        }
    }
}