{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Life Insurance",
    "description": "Data schema describing Inquiry",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "department": {
            "type": "object",
            "aiTitle": "department",
            "additionalProperties": false,
            "properties": {
                "code": {
                    "type": "string"
                },
                "nameLocalized": {
                    "type": "string"
                }
            }
        },
        "textOfInquiry": {
            "type": "string",
            "aiTitle": "textOfInquiry"
        },
        "textOfAnswer": {
            "type": "string",
            "aiTitle": "textOfAnswer"
        },
        "textOfComment": {
            "type": "string",
            "aiTitle": "textOfComment"
        },
        "quoteNumber": {
            "type": "string",
            "aiTitle": "quoteNumber"
        },
        "quoteId": {
            "type": "string",
            "aiTitle": "quoteId"
        },
        "configurationCodeName": {
            "type": "string",
            "aiTitle": "configurationCodeName"
        },
        "creatorUserName": {
            "type": "string"
        },
        "currentAssignee": {
            "type": "string"
        },
        "holder": {
            "type": "string",
            "aiTitle": "holder"
        },
        "policyReviewNumber": {
            "type": "number"
        },
        "insuredPerson": {
            "type": "string",
            "aiTitle": "insuredPerson"
        },
        "uwTriggers": {
            "$ref": "component:UWTriggers"
        },
        "riskPremium":{
            "type": "number",
            "aiTitle": "riskPremium"
        },
        "currencyDesc":{
            "type": "string",
            "aiTitle": "currencyDesc"
        }
    }
}