    "aiDataEnrichment": [
        {
            "name": "setPolicyTermsEnrichment"
        }
    ],
    "aiValidation": {
        "rule": "validationPolicyTerms"
    },
    "properties": {
        "startDate": {
            "type": "string",
            "aiFormat": "date",
            "aiTitle": "startDate"
        },
        "endDate": {
            "type": "string",
            "aiFormat": "date",
            "aiTitle": "endDate"
        },
        "effectiveDate": {
            "type": "string",
            "aiFormat": "date",
