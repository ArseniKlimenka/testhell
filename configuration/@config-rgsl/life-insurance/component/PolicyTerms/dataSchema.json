        "paymentPeriodEndDate": {
            "type": "string",
            "aiFormat": "date",
            "aiTitle": "paymentPeriodEndDate"
        },
        "accumulationPeriodStartDate": {
            "type": "string",
            "aiFormat": "date",
            "aiTitle": "accumulationPeriodStartDate"
        },
        "accumulationPeriodEndDate": {
            "type": "string",
            "aiFormat": "date",
            "aiTitle": "accumulationPeriodEndDate"
        },
        "payOutStartDate": {
            "type": "string",
            "aiFormat": "date",
            "aiTitle": "payOutStartDate"
        },
        "payOutEndDate": {
            "type": "string",
            "aiFormat": "date",
            "aiTitle": "payOutEndDate"
        },
        "paymentPeriodString": {
            "type": "string",
            "aiTitle": "paymentPeriodString"
        },
        "manualCorrection": {
            "type": "boolean",
            "aiTitle": "manualCorrection"
        },
        "paymentPeriodLastDate": {
            "type": "string",
            "aiFormat": "date",
            "aiTitle": "paymentPeriodLastDate"
        }
    }
