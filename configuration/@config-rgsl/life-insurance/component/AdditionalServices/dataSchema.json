{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Life Insurance",
    "description": "Data schema describing Additional Services",
    "type": "array",
    "additionalProperties": false,
    "aiDataEnrichment": [
        {
            "name": "setAdditionalServicesEnrichment"
        }
    ],
    "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
            "serviceCode": {
                "type": "string",
                "aiTitle": "serviceCode"
            },
            "serviceName": {
                "type": "string",
                "aiTitle": "serviceName"
            },
            "startDate": {
                "type": "string",
                "aiFormat": "date",
                "aiTitle": "startDate"
            },
            "endDate": {
                "type": "string",
                "aiFormat": "date",
                "aiTitle": "endDate"
            },
            "serviceParty": {
                "type": "string",
                "aiTitle": "serviceParty"
            },
            "serviceFrequency": {
                "type": "string",
                "aiTitle": "serviceFrequency"
            },
            "specialConditions": {
                "type": "string",
                "aiTitle": "specialConditions"
            },
            "serviceTypeCode": {
                "type": "string",
                "aiTitle": "serviceTypeCode"
            },
            "serviceType": {
                "type": "string",
                "aiTitle": "serviceType"
            },
            "serviceSubTypeCode": {
                "type": "string",
                "aiTitle": "serviceSubTypeCode"
            },
            "serviceSubType": {
                "type": "string",
                "aiTitle": "serviceSubType"
            },
            "serviceAgreement": {
                "type": "string",
                "aiTitle": "serviceAgreement"
            },
            "serviceStartDate": {
                "type": "string",
                "aiFormat": "date",
                "aiTitle": "serviceStartDate"
            },
            "serviceEndDate": {
                "type": "string",
                "aiFormat": "date",
                "aiTitle": "serviceEndDate"
            }
        }
