{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Life Insurance",
    "description": "Data schema describing recommended strategies",
    "type": "array",
    "additionalProperties": false,
    "items": {
        "type": "object",
        "additionalProperties": false,
        "properties": {
            "productCode": {
                "type": "string"
            },
            "strategyDescription": {
                "type": "string"
            },
            "strategyCode": {
                "type": "string"
            },
            "recommendationDateFrom": {
                "type": "string"
            },
            "recommendationDateTo": {
                "type": "string"
            },
            "recommendationText": {
                "type": "string"
            }
        }
    }
}