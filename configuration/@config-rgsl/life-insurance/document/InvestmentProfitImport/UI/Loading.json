{
    "id": "InvestmentProfitImportLoading",
    "type": "GridLayout",
    "formProperties": {
        "events": {
            "onLoad": "onLoadDocumentAction"
        }
    },
    "numberOfColumns": 4,
    "elements": [
        {
            "type": "Component",
            "name": "DocumentHeader",
            "value": {
                "$ref": "#"
            }
        },
        {
            "type": "Group",
            "width": 4,
            "elements": [
                {
                    "type": "GridLayout",
                    "numberOfColumns": 4,
                    "fillAvailableSpace": true,
                    "width": 3,
                    "elements": [
                        {
                            "type": "Group",
                            "elements": [
                                {
                                    "type": "TabLayout",
                                    "options": {
                                        "fixedPosition": false,
                                        "scrollableTabs": true
                                    },
                                    "elements": [
                                        {
                                            "type": "Tab",
                                            "elementId": "InvestmentProfitTab",
                                            "elements": [
                                                {
                                                    "type": "Section",
                                                    "label": "ImportInfo",
                                                    "elements": [
                                                        {
                                                            "type": "Group",
                                                            "elements": [
                                                                {
                                                                    "type": "FileUpload",
                                                                    "label": "FileSelection",
                                                                    "description": "FileSelectionDescription",
                                                                    "readOnly": true,
                                                                    "value": {
                                                                        "$ref": "#/properties/Body/properties/file"
                                                                    },
                                                                    "width": 1
                                                                }
                                                            ]
                                                        }
                                                    ],
                                                    "sectionLevel": 1
                                                }
                                            ],
                                            "title": "Upload data"
                                        },
                                        {
                                            "type": "Tab",
                                            "elementId": "loadingLoadedDataTabID",
                                            "elements": [
                                                {
                                                    "type": "Section",
                                                    "label": "LoadDataErrors",
                                                    "elementId": "loadingLoadedDataErrorsID",
                                                    "elements": [
                                                        {
                                                            "type": "InlineView",
                                                            "showContainerActions": false,
                                                            "views": [
                                                                {
                                                                    "viewReference": {
                                                                        "configurationCodeName": "InvestmentProfitLoadedDataErrorsFromExcel",
                                                                        "configurationVersion": "1",
                                                                        "configurationConceptType": "SearchView"
                                                                    },
                                                                    "onViewInitialized": "initLoadedDataFromExcel",
                                                                    "viewType": "Search"
                                                                }
                                                            ],
                                                            "value": {
                                                                "$ref": "#"
                                                            },
                                                            "hide": false,
                                                            "elementId": "InvestmentProfitImportInlineView"
                                                        }
                                                    ],
                                                    "sectionLevel": 1
                                                },
                                                {
                                                    "type": "Section",
                                                    "label": "LoadData",
                                                    "elementId": "importingLoadedDataID",
                                                    "elements": [
                                                        {
                                                            "type": "InlineView",
                                                            "showContainerActions": false,
                                                            "views": [
                                                                {
                                                                    "viewReference": {
                                                                        "configurationCodeName": "InvestmentProfitLoadedDataFromExcel",
                                                                        "configurationVersion": "1",
                                                                        "configurationConceptType": "SearchView"
                                                                    },
                                                                    "onViewInitialized": "initLoadedDataFromExcel",
                                                                    "viewType": "Search"
                                                                }
                                                            ],
                                                            "value": {
                                                                "$ref": "#"
                                                            },
                                                            "hide": false,
                                                            "elementId": "InvestmentProfitImportInlineView"
                                                        }
                                                    ],
                                                    "sectionLevel": 1
                                                }
                                            ],
                                            "title": "LoadedData"
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                },
                {
                    "type": "SideContentLayout",
                    "fixedPosition": true,
                    "width": 1,
                    "elements": [
                        {
                            "type": "GridLayout",
                            "numberOfColumns": 2,
                            "elements": [
                                {
                                    "type": "TabLayout",
                                    "elements": [
                                        {
                                            "type": "Tab",
                                            "icon": {
                                                "name": "Bars",
                                                "size": "Large"
                                            },
                                            "elements": [
                                                {
                                                    "type": "OutputText",
                                                    "label": "InvestmentProfit document number",
                                                    "skipTranslate": true,
                                                    "value": {
                                                        "$ref": "#/properties/Number"
                                                    },
                                                    "width": 2
                                                },
                                                {
                                                    "type": "OutputText",
                                                    "label": "Date",
                                                    "skipTranslate": true,
                                                    "template": "{{expr date 'd'}}",
                                                    "value": {
                                                        "$ref": "#/properties/Body/properties/summary/properties"
                                                    },
                                                    "width": 2
                                                }
                                            ],
                                            "elementId": "tab-Summary"
                                        },
                                        {
                                            "type": "Tab",
                                            "icon": {
                                                "name": "Clipboard-Check",
                                                "size": "Large"
                                            },
                                            "elements": [
                                                {
                                                    "type": "Component",
                                                    "name": "AuditInfo",
                                                    "value": {
                                                        "$ref": "#/properties/AuditInfo"
                                                    }
                                                }
                                            ],
                                            "elementId": "tab-Audit Info"
                                        },
                                        {
                                            "type": "Tab",
                                            "icon": {
                                                "name": "Bell",
                                                "size": "Large"
                                            },
                                            "tabType": "error",
                                            "elements": [
                                                {
                                                    "type": "ValidationSummary",
                                                    "elementId": "RequiredPropertiesValidations"
                                                }
                                            ],
                                            "elementId": "tab-Notifications",
                                            "additionalInfo": {
                                                "display": "ValidationErrorsCount"
                                            }
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            ]
        },
        {
            "type": "Component",
            "name": "DocumentFooter",
            "value": {
                "$ref": "#"
            }
        }
    ]
}