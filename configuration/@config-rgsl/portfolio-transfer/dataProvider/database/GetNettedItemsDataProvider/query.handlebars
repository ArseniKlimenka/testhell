SELECT isl.PORTFOLIO_TRANSFER_ITEM_HKEY
FROM ACC_IMPL.PORTFOLIO_TRANSFER_HUB hub
INNER JOIN ACC_IMPL.PORTFOLIO_TRANSFER_ITEM_LINK l
	ON l.PORTFOLIO_TRANSFER_HKEY = hub.PORTFOLIO_TRANSFER_HKEY
INNER JOIN ACC_IMPL.PORTFOLIO_TRANSFER_ITEM_SAT_LATEST isl
	ON isl.PORTFOLIO_TRANSFER_ITEM_HKEY = l.PORTFOLIO_TRANSFER_ITEM_HKEY
WHERE l.CONTRACT_NUMBER = @referenceNo
	AND hub.PORTFOLIO_TRANSFER_NUMBER = @ptNumber
	AND isl.IS_DELETED = 0