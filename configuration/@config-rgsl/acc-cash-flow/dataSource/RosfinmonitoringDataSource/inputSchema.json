{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "criteria": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "paymentStatusIds": {
                    "type": "array",
                    "items": {
                        "$ref": "component:PaymentStatusComponent"
                    }
                },
                "direction": {
                    "$ref": "component:DirectionComponent"
                },
                "transactionDateFrom": {
                    "type": "string",
                    "aiFormat": "date"
                },
                "amountFrom": {
                    "type": "number"
                }
            }
        }
    }
}
