{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "additionalProperties": false,
    "properties": {
        "aggregatedPaymentNumber": {
            "type": "string",
            "description": "Uploaded excel identifier"
        },
        "bankStatementNo": {
            "type": "string",
            "description": "Bank statement number to which this small payment is linked."
        },
        "smallPaymentNumber": {
            "type": "string",
            "description": "Separated (small) payment identifier"
        },
        "paymentDate": {
            "type": "string",
            "aiFormat": "date",
            "description": "Client payment date"
        },
        "payerFullName": {
            "type": "string",
            "description": "Payer full name"
        },
        "paymentDescription": {
            "type": "string",
            "description": "Contract number for which payment was done"
        },
        "contractBeginDate": {
            "type": "string",
            "aiFormat": "date",
            "description": "Contract begin date"
        },
        "productName": {
            "type": "string",
            "description": "Product name"
        },
        "paymentAmount": {
            "type": "number",
            "description": "Small payment amount"
        },
        "currencyCode": {
            "type": "string",
            "description": "Currency code"
        },
        "contractDuration": {
            "type": "string",
            "description": "Contract duration in years"
        },
        "additionalInformation": {
            "type": "string",
            "description": "Additional information"
        },
        "segment": {
            "type": "string",
            "description": "Segment"
        },
        "hasPayment": {
            "type": "string"
        }
    }
}