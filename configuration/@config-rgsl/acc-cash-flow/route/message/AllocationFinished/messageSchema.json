{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "allocationIds",
        "documentTypeId",
        "bankStatementItemId",
        "documentNo"
    ],
    "properties": {
        "allocationIds": {
            "type": "array",
            "items": {
                "type": "number"
            }
        },
        "documentTypeId": {
            "type": "integer"
        },
        "bankStatementItemId": {
            "type": "integer"
        },
        "documentNo": {
            "type": "string"
        }
    }
}