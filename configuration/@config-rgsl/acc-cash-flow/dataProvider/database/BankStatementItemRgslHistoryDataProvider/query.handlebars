select
	h.BANK_STATEMENT_ITEM_HISTORY_ID,
	u.USERNAME,
	h.CREATE_DATE,
	h.STATUS_ID_FROM,
	h.STATUS_ID_TO,
	h.PAYMENT_DESCRIPTION_FROM,
    h.PAYMENT_DESCRIPTION_TO
from
	acc_impl.BANK_STATEMENT_ITEM_HISTORY h
	inner join org.APPLICATION_USER u on u.APPLICATION_USER_ID = h.USER_ID
where 1=1
	and h.BANK_STATEMENT_ITEM_ID = @bankStatementItemId