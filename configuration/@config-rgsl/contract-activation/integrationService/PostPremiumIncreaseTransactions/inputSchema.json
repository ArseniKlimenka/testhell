{
    "$schema": "http://json-schema.org/draft-04/schema",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "contractNumbers"
    ],
    "properties": {
        "contractNumbers": {
            "type": "array",
            "description": "Contract number to post transactions for",
            "items": {
                "type": "string"
            }
        },
        "postingDateTo": {
            "type": "string",
            "aiFormat": "date"
        },
        "postingDescription": {
            "type": "string",
            "description": "Journal description. Can be used to avoid duplication of journals."
        }
    }
}