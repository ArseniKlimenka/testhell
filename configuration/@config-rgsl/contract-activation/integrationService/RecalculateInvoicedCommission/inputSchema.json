{
    "$schema": "http://json-schema.org/draft-04/schema",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "contracts"
    ],
    "properties": {
        "contracts": {
            "type": "array",
            "required": [
                "contractNumber"
            ],
            "items": {
                "type": "object",
                "properties": {
                    "contractNumber": {
                        "type": "string"
                    },
                    "repostFromDate": {
                        "type": "string",
                        "aiFormat": "date"
                    },
                    "repostFromBegining": {
                        "type": "boolean"
                    }
                }
            }
        }
    }
}