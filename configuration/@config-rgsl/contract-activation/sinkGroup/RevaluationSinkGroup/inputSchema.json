{
    "$schema": "http://json-schema.org/draft-04/schema",
    "type": "object",
    "additionalProperties": false,
    "required": [
        "contractNumbers",
        "newRevaluationDate"
    ],
    "properties": {
        "contractNumbers": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "newRevaluationDate": {
            "type": "string",
            "aiFormat": "date"
        },
        "postingDescription": {
            "type": "string"
        },
        "businessEventId": {
            "type": "string",
            "aiType": "guid"
        }
    }
}