{
    "$schema": "http://json-schema.org/draft-04/schema",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "contractNumber": {
            "type": "string",
            "description": "Contract number"
        },
        "contractNumbers":{
            "type": "array",
            "description": "Array of contract numbers to invoice",
            "items": {
                "type": "string"
            }
        },
        "postingDateTo": {
            "type": "string",
            "aiFormat": "date",
            "description": "Installments that will go to invoicing (used by installment based invoicing), usually current month"
        }
    }
}