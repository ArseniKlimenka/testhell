{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "criteria": {
            "type": "object",
            "additionalProperties": false,
            "required": [
                "contractNumbers"
            ],
            "properties": {
                "contractNumbers": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                },
                "skipFixedExchangeRate": {
                    "type": "boolean"
                },
                "skipCurrencies": {
                    "type": "array",
                    "items": {
                        "type": "string"
                    }
                }
            }
        }
    }
}