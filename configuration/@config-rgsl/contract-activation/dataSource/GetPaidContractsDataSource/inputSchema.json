    "type": "object",
    "additionalProperties": false,
    "properties": {
        "criteria": {
            "type": "object",
            "additionalProperties": false,
            "properties": {
                "currentStatus": {
                    "type": "string"
                },
                "contractNumbers": {
                    "type": "array",
