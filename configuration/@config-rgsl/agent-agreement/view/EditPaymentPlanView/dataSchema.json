{
    "$schema": "http://json-schema.org/draft-04/schema",
    "type": "object",
    "additionalProperties": false,
    "properties": {
        "paymentPlan":{
            "type": "array",
            "items": {
                "type": "object",
                "additionalProperties": false,
                "properties": {
                    "paymentPeriodStart": {
                        "type": "string",
                        "aiFormat": "date",
                        "aiTitle": "paymentPeriodStart"
                    },
                    "paymentPeriodEnd": {
                        "type": "string",
                        "aiFormat": "date",
                        "aiTitle": "paymentPeriodEnd"
                    },
                    "paymentExpirationDate": {
                        "type": "string",
                        "aiFormat": "date",
                        "aiTitle": "paymentExpirationDate"
                    },
                    "paymentGraceDateProlongation": {
                        "type": "string",
                        "aiFormat": "date",
                        "aiTitle": "paymentGraceDateProlongation"
                    }
                }
            }        
        }
    }
}