SELECT
	ou.ORGANISATION_UNIT_ID,
	ou.ORGANISATION_UNIT_CODE,
	ou.PARENT_ID,
	ouiSat.NAME,
	ouiSat.CODE,
	ouiSat.PARTNER_CODE
FROM ORG.ORGANISATION_UNIT ou
JOIN ORG_IMPL.ORGANISATION_UNIT_HUB ouHub ON ou.ORGANISATION_UNIT_CODE = ouHub.ORGANISATION_UNIT_CODE
JOIN ORG_IMPL.ORGANISATION_UNIT_INFO_SAT_LATEST ouiSat ON ouHub.ORGANISATION_UNIT_HKEY = ouiSat.ORGANISATION_UNIT_INFO_HKEY
